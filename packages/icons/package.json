{
  "name": "@lifterlms/icons",
  "version": "0.0.0",
  "description": "Icon library for LifterLMS",
  "author": "Team LifterLMS <dev@lifterlms.com>",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/gocodebox/lifterlms/tree/master/packages/icons",
  "keywords": [
    "lifterlms",
    "wordpress",
    "scripts",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gocodebox/lifterlms.git",
    "directory": "packages/icons"
  },
  "bugs": {
    "url": "https://github.com/gocodebox/lifterlms/labels/package%3A%icons"
  },
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wordpress/icons": "^6.1.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "react-snap": "^1.23.0"
  },
  "reactSnap": {
    "source": "docs",
    "destination": "docs/build"
  },
  "scripts": {
    "docgen": "browserify docs/app.js -p esmify > docs/bundle.js && react-snap && node docs/generate.js && rm -rf docs/build && rm docs/200.html && rm docs/bundle.js"
  }
}
